#include <iostream>
using namespace std;
int main()
{
	double A1, A2, B1, B2, C1, C2, x, y;
	cout << "System:" << endl;
	cout << "A1*x + B1*y = C1," << endl;
	cout << "A2*x + B2*y = C2," << endl;
	cout << "Enter A1, B1, C1" << endl;
	cin >> A1 >> B1 >> C1;
	cout << "Enter A2, B2, C2" << endl;
	cin >> A2 >> B2 >> C2;
	y = (C2 * A1 - A2 * C1) / (B2 * A1 - B1 * A2);
	x = (C1 - B1 * y) / A1;
	cout << "x = " << x << "; y = " << y;
	return 0;
}
