#include <iostream>
using namespace std;
int main()
{
	double a, b;
	double S, R, P, D;
	cin >> a >> b;
	S = a * a + b * b;
	R = a * a - b * b;
	P = a * a * b * b;
	D = a * a / (b * b);
	cout << "a * a + b * b = " << S << endl;
	cout << "a * a - b * b = " << R << endl;
	cout << "a * a * b * b = " << P << endl;
	cout << "a * a / (b * b) = " << D;
	return 0;
}
