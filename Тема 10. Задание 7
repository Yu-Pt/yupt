#include <iostream>
#include <clocale>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int a, b, c;
	cout << "Введите числa a, b и c" << endl;
	cin >> a >> b >> c;
	cout << "Справедливо ли, что существует треугольник со сторонами a, b, c?" << endl;
	if (a < b + c && b < c + a && c < a + b)
		cout << "Да";
	else
		cout << "Нет";
	return 0;
}
