#include <iostream>
#include <clocale>
#include <math.h>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int A;
	cout << "Введите число А" << endl;
	cin >> A;
	if (A < 10 && A % 2 == 0)
		cout << "А - однозначное четное число";
	if (A < 10 && A % 2 != 0)
		cout << "А - однозначное нечетное число";
	if (A > 9 && A < 100 && A % 2 == 0)
		cout << "А - двузначное четное число";
	if (A > 9 && A < 100 && A % 2 != 0)
		cout << "А - двузначное нечетное число";
	if (A > 99 && A % 2 == 0)
		cout << "А - трехзначное четное число";
	if (A > 99 && A % 2 != 0)
		cout << "А - техзначное нечетное число";
	return 0;
}
