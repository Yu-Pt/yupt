#include <iostream>
#include <math.h>
using namespace std;

int main()
{
    int x1, x2, y1, y2;
    double r, d;
    cin >> x1 >> y1 >> x2 >> y2;
    cout << "(" << x1 << ";" << y1 << ")" << endl;
    cout << "(" << x2 << ";" << y2 << ")" << endl;
    d = (x2-x1)*(x2-x1)+(y2-y1)*(y2-y1);
    r = sqrt(d);
    cout << r;
    return 0;
}
