#include <iostream>
#include <clocale>
using namespace std;
int main()
{
	setlocale(LC_ALL, "Russian");
	int A, B;
	cout << "Введите числa A и B" << endl;
	cin >> A >> B;
	if (A == B)
	   {A = 0;
		  B = 0;}
	else
	   {if (A > B)
			   B = A;
		  else
			   A = B;}
	cout << "A=" << A << endl;
	cout << "B=" << B;
	return 0;
}
