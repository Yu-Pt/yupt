#include <iostream>
#include <clocale>
#include <math.h>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int A;
	cin >> A;
	if (A == 10) cout << "Десять учебных заданий";
	if (A == 11) cout << "Одиннадцать учебных заданий";
	if (A == 12) cout << "Двенадцать учебных заданий";
	if (A == 13) cout << "Тринадцать учебных заданий";
	if (A == 14) cout << "Четырнадцать учебных заданий";
	if (A == 15) cout << "Пятнадцать учебных заданий";
	if (A == 16) cout << "Шестнадцать учебных заданий";
	if (A == 17) cout << "Семнадцать учебных заданий";
	if (A == 18) cout << "Восемнадцать учебных заданий";
	if (A == 19) cout << "Девятнадцать учебных заданий";
	if (A > 19 && A < 30) cout << "Двадцать ";
	if (A > 29 && A < 40) cout << "Тридцать ";
	if (A == 40) cout << "Сорок учебных заданий";
	if (A > 20 && A % 10 == 1) cout << "одно учебное задание";
	if (A > 20 && A % 10 == 2) cout << "два учебных задания";
	if (A > 20 && A % 10 == 3) cout << "три учебных задания";
	if (A > 20 && A % 10 == 4) cout << "четыре учебных задания";
	if (A > 20 && A % 10 == 5) cout << "пять учебных заданий";
	if (A > 20 && A % 10 == 6) cout << "шесть учебных заданий";
	if (A > 20 && A % 10 == 7) cout << "семь учебных заданий";
	if (A > 20 && A % 10 == 8) cout << "восемь учебных заданий";
	if (A > 20 && A % 10 == 9) cout << "девять учебных заданий";
	if (A == 20 || A == 30) cout << "учебных заданий";
return 0;
}
