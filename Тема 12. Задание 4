#include <iostream>
#include <clocale>
#include <math.h>
using namespace std;

int main()
{
	setlocale(LC_ALL, "Russian");
	int A;
	cin >> A;
	if (A > 99 && A < 200) cout << "Сто ";
	if (A > 199 && A < 300) cout << "Двести ";
	if (A > 299 && A < 400) cout << "Триста ";
	if (A > 399 && A < 500) cout << "Четыреста ";
	if (A > 499 && A < 600) cout << "Пятьсот ";
	if (A > 599 && A < 700) cout << "Шестьсот ";
	if (A > 699 && A < 800) cout << "Семьсот ";
	if (A > 799 && A < 900) cout << "Восемьсот ";
	if (A > 899 && A < 1000) cout << "Девятьсот ";
	if (A % 100 == 11) cout << "одиннадцать";
	if (A % 100 == 12) cout << "двенадцать";
	if (A % 100 == 13) cout << "тринадцать";
	if (A % 100 == 14) cout << "четырнадцать";
	if (A % 100 == 15) cout << "пятнадцать";
	if (A % 100 == 16) cout << "шестнадцать";
	if (A % 100 == 17) cout << "семнадцать";
	if (A % 100 == 18) cout << "восемнадцать";
	if (A % 100 == 19) cout << "девятнадцать";
	if ((A % 100) / 10 == 2) cout << "двадцать ";
	if ((A % 100) / 10 == 3) cout << "тридцать ";
	if ((A % 100) / 10 == 4) cout << "сорок ";
	if ((A % 100) / 10 == 5) cout << "пятьдесят ";
	if ((A % 100) / 10 == 6) cout << "шестьдесят ";
	if ((A % 100) / 10 == 7) cout << "семьдесят ";
	if ((A % 100) / 10 == 8) cout << "восемьдесят ";
	if ((A % 100) / 10 == 9) cout << "девяносто ";
	if (A % 100 > 20 && A % 10 == 1) cout << "один";
	if (A % 100 > 20 && A % 10 == 2) cout << "два";
	if (A % 100 > 20 && A % 10 == 3) cout << "три";
	if (A % 100 > 20 && A % 10 == 4) cout << "четыре";
	if (A % 100 > 20 && A % 10 == 5) cout << "пять";
	if (A % 100 > 20 && A % 10 == 6) cout << "шесть";
	if (A % 100 > 20 && A % 10 == 7) cout << "семь";
	if (A % 100 > 20 && A % 10 == 8) cout << "восемь";
	if (A % 100 > 20 && A % 10 == 9) cout << "девять";
return 0;
}
